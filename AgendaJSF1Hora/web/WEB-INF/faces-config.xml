<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>listContatos</from-outcome>
            <to-view-id>/listContato.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>busContatoNome</from-outcome>
            <to-view-id>/busContatoNome.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addContato</from-outcome>
            <to-view-id>/cadContato.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/cadContato.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cadIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/listContato.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>listIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/busContatoNome.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>buscIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>resBusca</from-outcome>
            <to-view-id>/resultadoBusca.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
